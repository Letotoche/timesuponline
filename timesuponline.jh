
DEFAULT_MIN_LENGTH = 4
DEFAULT_MAX_LENGTH = 50

entity Partie {
	nom String required maxlength(100)
}

entity Joueur {
	pseudo String required unique maxlength(50)
}


relationship ManyToMany {
	Partie{participant(pseudo)} to Joueur
}

relationship OneToMany {
	Partie{mdj(pseudo)} to Joueur
}

filter Joueur, Partie

paginate Joueur, Partie with pagination

service all with serviceImpl